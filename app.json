{
  "expo": {
    "name": "Fitness Journey",
    "slug": "hackathon-rn",
    "version": "1.0.0",
    "orientation": "portrait",
    "sdkVersion": "54.0.0",
    "userInterfaceStyle": "dark",
    "scheme": "hackathonrn",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#1a1a1a"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.hackathon.fitnessjourney"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#1a1a1a"
      },
      "package": "com.hackathon.fitnessjourney",
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "hackathonrn"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    }
  }
}

